{
    "name": "Redirector",
    "description": "",
    "version": "1",
    "permissions": [
        "storage",
        "declarativeNetRequestWithHostAccess",
        "activeTab",
        "declarativeContent",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "manifest_version": 3
}